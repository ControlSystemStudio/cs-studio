<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.csstudio.pvmanager.feature"
      label="CS-Studio Common Data Layer (pvmanager)"
      version="3.2.0.qualifier"
      provider-name="CS-Studio Collaboration &lt;cs-studio-core@lists.sourceforge.net&gt;">

   <description url="http://pvmanager.sourceforge.net/">
      pvmanager is the connection layer for Control System Studio. It provides support for datasources (real-time publish/subscribe protocols) and services (RPC and command/response protocols).
   </description>

