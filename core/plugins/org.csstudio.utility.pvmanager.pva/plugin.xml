<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.csstudio.utility.pvmanager.datasource">
      <datasource
            datasource="org.csstudio.utility.pvmanager.pva.PVADataSource"
            name="EPICS v4 DataSource (pva)"
            prefix="pva">
      </datasource>
   </extension>

   <extension
         point="org.csstudio.utility.pvmanager.servicefactory">
      <servicefactory
            name="PVARPCfactory"
            servicefactory="org.csstudio.utility.pvmanager.pva.CsstudioPVARPCServiceFactory">
      </servicefactory>
   </extension>

   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>

</plugin>
