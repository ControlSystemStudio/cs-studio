<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.csstudio.ui.menu.popup.processvariable">
         <command
               commandId="org.csstudio.phoebus.integration.pvtree.OpenPhoebusPVTree"
               icon="icons/pvtree.png"
               label="%PhoebusPVTreeLabel"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:utility">
         <command
               commandId="org.csstudio.phoebus.integration.pvtree.OpenPhoebusPVTree"
               icon="icons/pvtree.png"
               label="%PhoebusPVTreeLabel"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.phoebus.integration.pvtree.OpenPhoebusPVTree"
            icon="icons/pvtree.png"
            id="org.csstudio.phoebus.integration.pvtree.OpenPhoebusPVTree"
            name="PV Tree">
      </command>
   </extension>
</plugin>
