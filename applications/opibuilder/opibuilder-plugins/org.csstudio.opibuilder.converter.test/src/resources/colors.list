 >=1.5  && <2.5	 : "Monitor: NORMAL"   # open       = 10
 >=2.5  && <=3.5 : "Monitor: MAJOR"     # fault      = 11
}
rule 91 "SNS-Vlv-2" {
 >=-0.5 && <0.5	 : "Wid-alt/Anno-sec"    # transition = 00
