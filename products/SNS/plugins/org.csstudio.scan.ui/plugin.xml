<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:css?after=display">
         <menu
               icon="icons/scan.gif"
               id="scan"
               label="%Scan">
            <separator
                  name="additions"
                  visible="false">
            </separator>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.csstudio.scan.ui.PreferencePage"
            id="org.csstudio.scan.ui.prefpage"
            name="%Scan">
      </page>
   </extension>
</plugin>
