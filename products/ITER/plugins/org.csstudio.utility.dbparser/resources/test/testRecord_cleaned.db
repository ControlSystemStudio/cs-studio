
record(bo, "$(DEVICE):KEZSTAI-CCC") {				
  field(DTYP, "asynInt32")
  field(OUT, "@asyn($(DEVICE),0) start_stop_ai")
  field(ZNAM, "Stop")
  field(ONAM, "Start")
}

record(ao, "$(DEVICE):KEZPUAI-CCC") {				
  field(DTYP, "asynInt32")
  field(OUT, "@asyn($(DEVICE),0) pulse_ai")
}

