<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.csstudio.archive.reader.ArchiveReader">
      <ArchiveReader
            class="org.csstudio.archive.reader.kblog.KBLogArchiveReaderFactory"
            name="org.csstudio.archive.reader.kblog.archivereader"
            prefix="kblog">
      </ArchiveReader>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.platform.ui.css.applications.trends"
            class="org.csstudio.archive.reader.kblog.KBLogPreferencePage"
            id="org.csstudio.archive.reader.kblog.preferences"
            name="%prefpage.name">
      </page>
   </extension>

</plugin>
